{
    "name": "daikon/data-structures",
    "type": "library",
    "description": "Immutable data-structure traits based on php/ds.",
    "keywords": [ "list", "map", "data-structures", "immutable" ],
    "homepage": "https://github.com/daikon-cqrs/data-structures",
    "license": "MPL-2.0",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "php-ds/php-ds": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "satooshi/php-coveralls": "master@dev",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": { "Daikon\\DataStructures\\" : "src" }
    },
    "autoload-dev": {
        "psr-4": { "Daikon\\Tests\\DataStructures\\": "tests" }
    },
    "scripts": {
        "build": [
            "@test",
            "@code-sniffer",
            "@validate-json"
        ],
        "test": "phpunit",
        "code-sniffer": "phpcs --extensions=php --standard=psr2 src tests",
        "code-sniffer-autofix": "phpcbf --standard=psr2 -d tabWidth=4 src",
        "validate-json": "composer validate --no-check-all --no-check-lock",
        "validate-deps": "composer validate --with-dependencies --strict"
    }
}
